<UserControl x:Class="C19K.Wpf.CustomControls.SingleColumnSeries"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:C19K.Wpf.CustomControls"
             xmlns:oxy="http://oxyplot.org/wpf"
             mc:Ignorable="d" 
             Name="ThisControl"
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <oxy:PlotView Model="{Binding ElementName=ThisControl, Path=CurrentPlotModel, UpdateSourceTrigger=PropertyChanged}" Height="500" Width="500" Background="AliceBlue" />
        
    </Grid>
</UserControl>
